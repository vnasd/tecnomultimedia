float s= 1.2;

void cuadrado(){
size (800,400);
rectMode(CENTER);

  background(255);
  translate (600,200);
  noFill();
  stroke(r, g, b);
  strokeWeight(1*s);
  rect(0,0,30,30);
  scale(s);
  
  noFill();
  strokeWeight(1*s);
  rect(0,0,40,40);
  scale(s);
  
  noFill();
  strokeWeight(1*s);
  rect(0,0,50,50);
  scale(s);
  
  noFill();
  strokeWeight(1.5*s);
  rect(0,0,60,60);
  scale(s);
  
  noFill();
  strokeWeight(2*s);
  rect(0,0,70,70);
  scale(s);
  
  noFill();
  strokeWeight(2.5*s);
  rect(0,0,80,80);
  scale(s);
  
  noFill();
  strokeWeight(3*s);
  rect(0,0,90,90);
  scale(s);
  
  noFill();
  strokeWeight(3.5*s);
  rect(0,0,100,100);
  scale(s);
  
  noFill();
  strokeWeight(4*s);
  rect(0,0,110,110);
  scale(s);
}
